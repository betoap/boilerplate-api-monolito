  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "npm run dev node ./dist/bin/clusters.js",
    "start:dev": "npm run build && npm run copy && npm run dev npm run dev:run",
    "copy": "rm dist/config -r -f && cp src/config dist/config -r",
    "dev": "NODE_ENV=development HTTP_PORT=3000 HTTPS_PORT=3001 HTTP2_PORT=3002 npm run dev:cert npm run dev:db",
    "dev:cert": "HTTPS_KEY=server.key HTTPS_CERTIFICATION=server.crt HTTP2_KEY=localhost-privkey.pem HTTP2_CERTIFICATION=localhost-cert.pem",
    "dev:db": "DATABASE_SQL_HOST=localhost DATABASE_SQL_NAME=artecomcarinho DATABASE_SQL_USER=root DATABASE_SQL_PASSWORD=admin DATABASE_SQL_LOGGING=true DATABASE_SQL_DIALECT=mariadb",
    "dev:run": "nodemon ./dist/bin/clusters --nolazy",
    "prd": "npm run build && npm run copy"
  },
